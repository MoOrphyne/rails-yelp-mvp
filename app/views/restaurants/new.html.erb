 <p></p>
  <div class="container border rounded col-xs-12 col-sm-9 col-md-6">
    <h1>Create a new restaurant</h1>
      <%= simple_form_for @restaurant do |f| %>
        <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :phone_number %>
        <%= f.input :category, collection: Restaurant::CATEGORIES %>
        <%= f.submit class: 'btn btn-primary' %>
      <% end %>
    <div>
      <p></p>
      <p><%= link_to "Back to List", restaurants_path(@restaurants) %></p>
    </div>
  </div>


